    'revision_uid' => '2',
    'biblio_type' => '102',
    'biblio_number' => NULL,
    'biblio_other_number' => NULL,
    'biblio_sort_title' => 'iHaemodipsus leporisi Blagoveshtchensky 1966 Phthiraptera Anoplu',
    'biblio_secondary_title' => 'Turkish Journal of Parasitology',
    'biblio_tertiary_title' => NULL,
    'biblio_edition' => NULL,
    'biblio_publisher' => NULL,
