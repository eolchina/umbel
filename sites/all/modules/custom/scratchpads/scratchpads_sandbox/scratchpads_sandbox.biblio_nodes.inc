    'biblio_sort_title' => 'Systematic review of endemic Sulawesi squirrels Rodentia Sciurid',
    'biblio_secondary_title' => 'Bulletin of the American Museum of Natural History',
    'biblio_tertiary_title' => NULL,
    'biblio_edition' => NULL,
    'biblio_publisher' => NULL,
