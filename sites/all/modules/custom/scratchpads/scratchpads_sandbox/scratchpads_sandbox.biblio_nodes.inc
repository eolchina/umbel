  (object)array(
    'uid' => '2',
    'title' => 'Ein Phänomen besonders bei Vögeln: Trinken von Tränenflüssigkeit (Lachrymophagie) durch Tierläuse (Insecta, Phthiraptera)*',
    'log' => '',
