    'biblio_sort_title' => 'Ein Phänomen besonders bei Vögeln Trinken von Tränenflüssigkeit ',
    'biblio_secondary_title' => 'Vogelwarte',
    'biblio_tertiary_title' => NULL,
    'biblio_edition' => NULL,
