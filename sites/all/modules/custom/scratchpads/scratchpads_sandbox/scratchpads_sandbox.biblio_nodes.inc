    'biblio_sort_title' => 'Parasitismo de iGallus Gallusi Linnaeus 1758 por espécies de Pht',
    'biblio_secondary_title' => 'Arquivos do Instituto Biológico',
    'biblio_tertiary_title' => NULL,
